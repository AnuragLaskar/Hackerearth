#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    while(n)
    {
        int sh,sm,eh,em,th,tm,st,et;
        scanf("%d %d %d %d",&sh,&sm,&eh,&em);
        st=sh*60+sm;
        et=eh*60+em;
        th=(et-st)/60;
        tm=(et-st)%60;
        printf("\n%d %d",th,tm);
        n--;
    }
    return 0;
}
