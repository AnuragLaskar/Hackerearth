#include<stdio.h>
#include<string.h>
int main()
{
    char str[100];
    int len,i;
    scanf("%s",str);
    len=strlen(str);
    for(i=0;i<len;i++)
    {
        if(str[i]>='A' && str[i]<='Z')
        {
            str[i]=str[i]+32;
            
        }
        else
        {
            str[i]=str[i]-32;
        }
    }
    printf("\n%s",str);
    return 0;
}
